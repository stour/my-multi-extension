{
  "type": "codenvy_extension",
  "builders": {
    "default": "maven"
  },
  "runners": {
    "default": "project:/java-console-runner",
    "configs": {
      "project:/java-console-runner": {
        "variables": {
          "configurationType": "java/standalone/simple"
        },
        "ram": 1000
      }
    }
  },
  "attributes": {
    "language": ["java"],
    "languageVersion": ["1.8"]
  }
}
